package com.Mphasis.MyDemoProject;

public class values {
	
	public int a;
	public int b;
	public int c;
	
	
	values(int i,int j,int k){
		this.a=i;
		this.b=j;
		this.c=k;
	}
	
	public int Add(int i,int j,int k) {
		int sum=i+j+k;
		return sum;
	}
	
	public int Avg(int sum) {
		int average=sum/2;
		return average;
		
	}
}
