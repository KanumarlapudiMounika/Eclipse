package com.Mphasis.MyDemoProject;

import java.util.Scanner;

public class arrstr {

	public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
		
		String[] ar=new String[3];
		for(int i=0;i<ar.length;i++) {
			 ar[i]=sc.next();
		}
		for(int i=0;i<ar.length;i++) {
			System.out.println(ar[i]);
		}
		for(String i : ar) {
			System.out.print(i + " ");
		}
		sc.close();
	}

}
